										<div class="form-group">
											<label for="category">Link Icon</label>
											<select class="form-control" name="link-icon" id="link-icon" placeholder="select your link icon">
												<option value="javascript">Facebook</option>
												<option value="php">Paypal</option>
												<option value="golong">Stackoverflow</option>
												<option value="ruby">Dribbble</option>
											</select>
										</div>
									
										</div>
										<div class="card-action">
											<button id="addCategory" class="btn btn-success">Add Category</button>
											<button class="btn btn-danger">Cancel</button>
										</div>
									</div>
								</div>




